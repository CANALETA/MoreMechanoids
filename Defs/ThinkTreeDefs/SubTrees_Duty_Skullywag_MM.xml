<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <!--============= AI Duty ============-->
  <ThinkTreeDef>
    <defName>SquadBrainDuty_Skullywag</defName>
	<thinkRoot Class="ThinkNode_Priority">
      <subNodes>
		<!--li Class="MoreMechanoids.JobGiver_FixAttackTargets"/-->
		<li Class="MoreMechanoids.ThinkNode_ConditionalNonPlayerFaction">
		  <subNodes>
		    <li Class="MoreMechanoids.JobGiver_DownAllHumans"/>
		  </subNodes>
		</li>

        <li Class="ThinkNode_ConditionalHasLord">
          <subNodes>
            <!--li Class="ThinkNode_Duty"/-->

            <!-- If no duty issued a job -->
            <!-- Wander at fallback -->
            <li Class="ThinkNode_ConditionalHasDutyTarget" >
              <subNodes>
                <li Class="JobGiver_WanderNearDutyLocation" />
              </subNodes>
            </li>

            <li Class="JobGiver_WanderAnywhere" />
            <li Class="JobGiver_IdleError" />
          </subNodes>
        </li>
      </subNodes>
    </thinkRoot>
  </ThinkTreeDef>





</Defs>


